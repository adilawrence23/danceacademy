extends base.pug

block scripts
  script(src='/static/index.js')

block style
    style
      include ../static/style.css
      include ../static/styleContact.css
block content
    div.container
        h1 Join Us..
        form(action="/contact", method="post", class="myForm")
            input(type="text", class="myInput",name="name", placeholder="Enter your Name")
            input(type="text", class="myInput",name="age", placeholder="Enter your Age")
            input(type="text", class="myInput",name="gender", placeholder="Gender")
            input(type="phone", class="myInput",name="phone", placeholder="Enter your Phone number")
            input(type="email", class="myInput",name="email", placeholder="Enter your Email Id")
            input(type="text", class="myInput",name="qualification", placeholder="Your Highest Qualification ")
            input(type="text", class="myInput",name="address", placeholder="Your Location")
            button(class="btn")
                |  #[a(href="/") Submit]